<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- declare-styleable: 自定义属性的值 -->
    <!-- "reference" //引用  -->
	<!-- "color" //颜色 -->
	<!-- "boolean" //布尔值   --> 
	<!-- "dimension" //尺寸值   -->
	<!-- "float" //浮点值   -->
	<!-- "integer" //整型值 -->
	<!-- "string" //字符串   -->
	<!-- "fraction" //百分数,比如200% --> 
    <declare-styleable name="TopTitle">
        <attr name="titleText" format="string"/>
        <attr name="leftSrc" format="reference|integer"/>
        <attr name="rightSrc" format="reference|integer"/>
    </declare-styleable>
</resources>